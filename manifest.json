{
	"name": "Leadswami Extension",
	"version": "1.0",
	"description": "Scrap profiles in linkedin!",
	"permissions": ["activeTab","declarativeContent","storage"],
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "images/get_started128.png"
		}
	},
	"icons": {
		"128": "images/get_started128.png"
	},
	"manifest_version": 2
}