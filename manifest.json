{
	"name": "Linkedin Form Extension",
	"version": "1.1",
	"description": "Linkedin Form",
	"permissions": [
		"activeTab",
		"browsingData",
		"storage",
		"https://*.linkedin.com/*"
	],
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "images/get_started128.png"
		}
	},
	"icons": {
		"128": "images/get_started128.png"
	},
	"manifest_version": 2
}